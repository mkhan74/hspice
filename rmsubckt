#!/bin/bash
#This is a shell script to remove the subcircuit simulation files
#generated by HSPICE.It also removes temporary and backup files.

#-----------------------------------------
#Author -Marouf Khan
#-----------------------------------------

find . -name '*.out' -type f -print -delete
find . -name '*.lis' -type f -print -delete
find . -name '*.ic*' -type f -print -delete
find . -name '*.st*' -type f -print -delete
find . -name '*.tr*' -type f -print -delete
find . -name '*.sw*' -type f -print -delete
find . -name '*.ac*' -type f -print -delete
find . -name '*.pa*' -type f -print -delete
find . -name '*.log' -type f -print -delete
find . -name '*.bak' -type f -print -delete

